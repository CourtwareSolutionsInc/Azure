@model AttachmentFieldModel

<input type="file" data-input-type="attachment" title="@Model.Tooltip" id="@Model.Id" name="@Model.Id" class="inline-editing @Model.Classes  @(Model.Required ? "required" : "")"@if(Model.Multiple) { <text>multiple='multiple'</text> } @if(Model.Width > 0) { <text>style='width:@Model.Width;'</text> }/>